{
  "version": 2,
  "builds": [
    {
      "src": "packages/trader/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "cd packages/trader && npm install && npx webpack --config build/webpack.config.js --env production",
  "installCommand": "npm install --workspace packages/trader",
  "outputDirectory": "packages/trader/dist",
  "framework": null
}
