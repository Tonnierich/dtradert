{
  "version": 2,
  "builds": [
    {
      "src": "packages/trader/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "cd packages/trader && npm install --legacy-peer-deps",
  "buildCommand": "cd packages/trader && npm run build",
  "outputDirectory": "packages/trader/dist",
  "framework": null
}
